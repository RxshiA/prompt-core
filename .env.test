# Test Environment Variables
# Copy from .env and modify for testing

# Server Configuration
PORT=0
NODE_ENV=test

# Frontend URL for CORS (not needed for tests)
FRONTEND_URL=http://localhost:3000

# Python Configuration
PYTHON_PATH=python3

# OpenAI Configuration (use your real API key for integration tests)
# For unit tests, this can be a dummy value
OPENAI_API_KEY=your_openai_api_key_here

# Test-specific Configuration
DEBUG=false
TEST_TIMEOUT=30000

# Rate Limiting (more lenient for tests)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=200
